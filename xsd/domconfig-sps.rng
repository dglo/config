<!-- mpeTriggerDiscriminator max/min = 971 / 546
     with slop max/min = 1068 / 491
    <xs:minInclusive value="491"/>

NOTE: According to Mark K -32768 is NOT a valid value here.
It gets set by domcal, but is a garbage value.  His preference
is to flag this value as an error.
-->
<define name="mpeTriggerDiscriminatorType">
  <data type="integer">
    <param name="maxInclusive">1068</param>
    <param name="minInclusive">491</param>
  </data>
</define>

