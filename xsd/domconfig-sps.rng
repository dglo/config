<!-- pmtHighVoltage max/min = 3374 / 0 
     with slop max/min = 3711/0

     As per Mark K. this parameter is in 1/2 volt counts.
     The upper limit is to be set to never allow the voltage to 
     be over 1900 volts.  1900/0.5 = 3800
-->
<define name="pmtHighVoltageType">
  <data type="integer">
    <param name="maxInclusive">3800</param>
    <param name="minInclusive">0</param>
  </data>
</define>

